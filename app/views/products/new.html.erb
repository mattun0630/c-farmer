<h2>商品新規追加</h2>
<%= form_for(@product, url: products_path) do |f| %>
	<%= f.label :product_image %><br>
    <%= f.file_field :product_image %><br>
    <%= f.label :product_name %><br>
    <%= f.text_field :product_name %><br>
    <%= f.label :detail %><br>
    <%= f.text_field :detail %><br>
    <%= f.submit '保存' %>
<% end %>
<h３>基本情報変更</h３>
	<%= form_for(@producer) do |f| %>
    <h5>事業者名</h5>
    <%= f.text_field :producer_name %>

	<h5>郵便番号</h5>
	<%= f.text_field :post %>

	<h5>住所</h5>
	<%= f.text_field :a_address %>

	<h5>電話番号</h5>
	<%= f.text_field :phone %>

    <h5>メールアドレス</h5>
    <%= f.text_field :email %>

	<%= f.submit '基本情報を変更する' %>
<% end %>

<%= link_to '生産者メニューへ', producer_path(@producer) %>
<h1>New Map</h1>

<%= form_for(@map, url: producers_maps_path) do |form| %>

  <div class="field">
    <%= form.label :address %>
    <%= form.text_field :address %>
  </div>

  <div class="field">
    <%= form.label :comment %>
    <%= form.text_area :comment %>
  </div>

  <div class="field">
    <%= form.label :lonlat %>
    <%= form.text_field :lonlat %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>


<%#= link_to 'Back', producers_maps_path %>


  <%= form_for(@post, url: posts_path)  do |f| %>

    <h4>投稿内容</h4>
      <p><%= f.radio_button :title, "質問" %>質問</p>
      <p><%= f.radio_button :title, "届いたよ！" %>届いたよ！</p>
      <p><%= f.radio_button :title, "メッセージ" %>メッセージ！</p>

    <h4>本文</h4>
      <%= f.text_area :description %>


    <%= f.submit '投稿' %>

  <% end %>
